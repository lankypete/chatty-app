{
  "name": "react-simple-boilerplate",
  "version": "1.0.0",
  "description": "Simple and lightweight Boilerplate for ReactJS projects",
  "scripts": {
    "lint": "eslint --ext .jsx,.js src",
    "start": "node server.js",
    "watch:css": "chokidar 'styles/**/*.css' -c 'npm run css:postcss:dev'",
    "css:postcss:dev": "postcss styles/styles.css -c ./postcss.config.js -o dist/styles/styles.css",
    "develop": "run-p watch:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noahlotzer/react-simple-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "reload",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Noah-Jerome Lotzer <n.lotzer@neusta.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noahlotzer/react-simple-boilerplate/issues"
  },
  "homepage": "https://github.com/noahlotzer/react-simple-boilerplate",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-loader": "6.3.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "chokidar-cli": "^1.2.0",
    "clean-css": "^4.1.9",
    "concat-cli": "^4.0.0",
    "critical": "^0.9.1",
    "css-loader": "0.26.1",
    "csscomb": "^4.2.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "node-sass": "4.5.0",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-load-config": "^1.2.0",
    "precss": "^2.0.0",
    "rework-npm-cli": "^0.1.3",
    "sass-loader": "6.0.0",
    "sockjs-client": "^1.1.2",
    "style-loader": "0.13.1",
    "uncss": "^0.15.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "dependencies": {
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "uuid": "^3.1.0"
  }
}
